{
  "compilerOptions": {
    "outDir": "build",
    "module": "commonjs",
    "moduleResolution": "node",
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "sourceMap": true,
    "lib": [
      "dom",
      "es2015"
    ],
    "baseUrl": "src",
    "paths": {
      "@models/*" : ["models/*"],
      "@services/*" : ["services/*"]
    }
  }
}